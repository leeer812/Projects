        <property name="BUTTON_ID" value="buttonOne"/>
        <property name="RANDOM_CLASS_RADIO" value="Random Classification"/>
        <property name="RANDOM_CLUSTER_RADIO" value="Random Clustering"/>
        <property name="DATA_LABEL_INSTANCE_LABEL" value="There are %d instances, %d labels."/>
