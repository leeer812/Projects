        <property name="DUP_NAME_TITLE" value="Duplicate Name"/>
        <property name="LOAD_TITLE" value="Load File"/>
        <property name="LOAD_ERROR_TITLE" value="Loading Errors"/>
        <property name="ALG_CONFIG_WINDOW_TITLE" value="Algorithm Configuration Window"/>

        <!-- APPLICATION-SPECIFIC MESSAGES -->
        <property name="SAVE_UNSAVED_WORK" value="Would you like to save current work?"/>
