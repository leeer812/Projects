        <property name="ALG_NAME_PACKAGE" value="algorithms.type.names"/>
        <property name="GET_MIN_LABELS_METHOD" value="getMinLabels"/>
        <property name="GET_MIN_INSTANCES_METHOD" value="getMinInstances"/>
        
    </property_list>
    <property_options_list/>
</properties>