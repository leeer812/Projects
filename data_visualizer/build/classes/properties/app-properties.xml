        <property name="INVALID_CONFIG_UPDATE" value="You must input a number above 0 and below 2,147,483,648 for update interval"/>
        
        <!-- APPLICATION-SPECIFIC PARAMETERS -->
        <property name="DATA_FILE_EXT" value="*.tsd"/>
        <property name="PICTURE_FILE_EXT" value="*.png"/>
